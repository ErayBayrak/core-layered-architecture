@using EntityLayer.Concrete
@model List<About>
@foreach (var x in Model)
{

        <h1 class="mb-0">
            @x.Name
            <span class="text-primary">@x.Surname</span>
        </h1>
        <div class="subheading mb-5">
            @x.Address · @x.PhoneNumber ·
            <a href="mailto:name@email.com">@x.Mail</a>
        </div>
        <p class="lead mb-5">@x.Description</p>
        
    }